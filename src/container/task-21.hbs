<div>

    <div class="card">
        <div class="card-body">
            {{#with heading}}
            <div class="card">
                <div class="card-body">

                    {{#with main}}

                    <h1 class="mb-3">{{title}}</h1>
                    
                    <p>{{text}}</p>
                    {{#with button_text}}
                    <div class="mb-3"><button type="button" class="btn btn-primary ">{{solid}}</button></div>
                   
                    <div><button type="button" class="btn btn-outline-primary">{{outline}}</button></div>
                    {{/with}}
                    {{/with}}


                </div>
            </div>

           
            <div class="card mt-3">
                <div class="card-body">

                    {{#with second}}

                    <h2 class="mb-3">{{title}}</h2>
                   
                    <p>{{text}}</p>
                    {{#with button_text}}
                   <div class="mb-3"> <button type="button" class="btn btn-secondary">{{solid}}</button></div>
                                    
                    <div><button type="button" class="btn btn-outline-secondary">{{outline}}</button></div>
                    {{/with}}
                    {{/with}}


                </div>
            </div>
            
            <div class="card mt-2">
                <div class="card-body">

                    {{#with subblock}}

                    <h3 class="mb-3">{{title}}</h3>
                   
                    <p>{{text}}</p>
                    {{#with button_text}}
                   <div class="mb-3"><button type="button" class="btn btn-dark">{{solid}}</button></div> 
                    
                    <div><button type="button" class="btn btn-outline-dark">{{outline}}</button></div>
                    {{/with}}
                    {{/with}}
                </div>
            </div>
        </div>
    </div>
    {{/with}}
   


    <div class="card mt-3">
        <div class="card-body">
            {{#with nested_display}}
            <div class="display-1">{{title}}</div>
            
            {{#with children_display}}
            <div class="card mt-3">
                <div class="card-body">
                    <div class="display-4"></div>
                    <p class="mb-2">{{text}}</p>
                   
                    {{#with button}}
                    <button type="button" class="btn btn-outline-warning">{{text}}</button>
                    {{/with}}

                </div>
                {{/with}}
            </div>
           
            <h6 class="mt-3">{{header_text}}</h6>
            {{#with button}}
            
            <div class="mb-3"><button type="button" class="btn btn-info mt-3">{{text}}</button></div>
            {{/with}}
            
            {{/with}}
        </div>
    </div>
   
    <div class="card mt-3">
        <div class="card-body">
            {{#with paragraph}}
            <p class="text-uppercase">{{title}}</p>
            <p class="text-lowercase">{{text}}</p>
            {{#with button}}
            <a href="url" type="button" class="btn btn-link">{{text}}</a>
            {{/with}}

            <div class="card">
                <div class="card-body">
                    <ul>
                        {{#each list_name}}
                        <li>
                            <a href="url" target="_blank">{{text}}</a>
                        </li>

                        {{/each}}
                    </ul>
                </div>
            </div>

            {{/with}}
        </div>



    </div>

</div>