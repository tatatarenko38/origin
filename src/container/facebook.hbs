<div>
    <h1>{{name}}</h1>
    <hr class="mb-3">
    
    <hr>

    <ul>

        {{#each users}}
        {{> name}}
        <hr class="mt-3">
        
        <h3>friends</h3>
        <ul>
            {{#each friends}}
            <li>
                {{> name}}
                <h4> mutual friends</h4>
                <ul>
                    {{#each mutual_friends}}
                    <li>
                        {{> name}}
                    </li>

                    {{/each}}
                </ul>




               
                <h3 class="mt-3">groups</h3>
                <ul>
                    {{#each groups}}
                    <li>
                        <p> ID: {{id}}</p>
                        <h3>{{name}}</h3>
                        <p>description: <q>{{description}}</q></p>
                        <h4>members:</h4>
                        <ul>
                            {{#each members}}
                            <li>
                                {{> name}}
                            </li>
                            {{/each}}
                        </ul>

                    </li>

                    {{/each}}
                </ul>

            </li>
            {{/each}}
        </ul>

        {{/each}}
    </ul>

</div>