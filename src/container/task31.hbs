<div class="m-5">

    {{#with navigation}}

    <nuv>
        {{#each links}}
        <a href="href" target="_blank">{{text}}</a>
        {{/each}}
        {{#with button}}
        <a href="href" type="button" class="btn btn-primary">{{text}}</a>
        {{/with}}
    </nuv>
    {{/with}}
    <hr class="mx-3">
    <div>
        {{#with header}}
        <h1>{{title}}</h1>
        <p>{{description}}</p>
        {{#with button}}
        <a href="link" type="button" class="btn btn-primary">{{text}}</a>
        {{/with}}
        {{/with}}
    </div>
    <hr class="mx-3">

    <div>
        {{#with city}}
        <h2>{{title}}</h2>
        <br>
        <br>

        <div class="row gy-4 gx-2">
            {{#each cards}}
            <div class="col-5">
                <div class="card">
                    <div class="card-body">

                        {{#with picture}}
                        <img src="{{src}}" alt="{{alt}}" class="card-img-top">
                        {{/with}}
                        <h2 class="card-title ">{{title}}</h2>
                        <p>{{description}} </p>
                        {{#with button}}
                        <a href="{{href}}" type="button" class="btn btn-primary">{{text}}</a>
                        {{/with}}

                    </div>
                </div>
            </div>
            {{/each}}
        </div>
        {{/with}}
    </div>
    <hr>

    <div>
        {{#with winner}}
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table">

            <thead>

                <tr>
                    {{#each head}}
                    <th>

                        {{this}}

                    </th>
                    {{/each}}
                </tr>

            </thead>


            <tbody>
                {{#each user}}
                <tr>
                    <th>{{place}}</th>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>{{nickname}}</td>
                </tr>
                {{/each}}
            </tbody>

            {{/with}}
            {{/with}}

        </table>
    </div>

     <hr class="mx-3">
    {{#with gallery}}
    <h2>{{title}}</h2>

    <div class="row">
        {{#each cards}}
        <div class="card col-4">

            <div class="card-header">
                {{name}}
            </div>
            {{#each items}}
            <ul class="list-group">
                <li class="list-group-item">
                    {{this}}
                </li>
            </ul>
            {{/each}}
        </div>
        {{/each}}
    </div>
    {{/with}}
    <hr class="mx-3">
    {{#with relaxion}}
    <div>
        <div class="display-3 mb-3">{{title}}</div>
        
        <div class="row gy-5">
            {{#each cards}}
            <div class="col-5">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <div class="card-title text-secondary">{{ title}}</div>
                        <p>{{ description}}</p>
                    </div>

                </div>

            </div>
            {{/each}}
        </div>
    </div>
    {{/with}}
    <hr class="mx-3">

    <div class="row">
        {{#with rest}}
        {{#each cards}}

        <div class="col-6">
            <div class="card text-center">
                <div class="card-header">
                    {{header}}
                </div>
                <div class="card-body">
                    <div class="card-title">{{title}}</div>
                    <p class="card-text">{{description}}</p>
                    <a href="#" type="button" class="btn btn-primary">{{button}}</a>
                </div>
                <div class="card-footer">
                    {{footer}}
                </div>
            </div>
        </div>

        {{/each}}
        {{/with}}
    </div>
    <hr class="mx-3">
    <div class="row">
        {{#with proposition}}
        <div class="display-2">{{title}}</div>
        <br>
        <br>
        {{#each card}}
        <div class="col-5">
            <div class="card text-bg-warning">
                <div class="card-header">{{header}}</div>
                <div class="card-body">
                    <div class="card-title">{{title}}</div>
                    <p>{{description}}</p>
                </div>
            </div>
        </div>
        {{/each}}
        {{/with}}
    </div>
    <hr class="mx-3">
    <div class="row g-5">
        {{#with footer}}
        <div class="col-3">
            <h5>{{title}}</h5>         
           
        {{/with}}
    </div>
</div>