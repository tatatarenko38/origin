<div>
    <h1>{{name}}</h1>
    <p><b>{{description}}</b></p>
 

    {{#with history}}
    <b>{{year}}, {{founder}}</b>
    {{/with}}
    <br>
    <hr>
    <hr>

    <h2>Types</h2>
    <ul>
        {{#each  types}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <br>
    <hr>

<h2>Syntax</h2>

{{#with syntax}} 
<h3>Variables</h3>  
<ul>
    {{#each variables}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<h3>Functions</h3>  
<ul>
    {{#each functions}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<h3>Conditionals</h3>  
<ul>
    {{#each conditionals}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<h3>Loops</h3>  
<ul>
    {{#each loops}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<h3>Classess</h3>  
<ul>
    {{#each classes}}
    <li>{{this}}</li>
    {{/each}}
</ul>
{{/with}}
<br>
<hr>
<h2>Features</h2>  
<ul>
    {{#each features}}
    <li>{{this}}</li>
    {{/each}}
</ul>
<br>
<hr>
<h2>Libraries</h2>  
<ul>
    {{#each libraries}}
  <li>
{{this}}
  </li>
    {{/each}}
</ul>

<br>
<hr>
<h2>Tools</h2>  
<ul>
    {{#each tools}}
  <li>
{{this}}
  </li>
    {{/each}}
</ul>

<br>
<hr>
<h3>Community</h3>
<ul>
    {{#each community}}
    <li>
        <button type="button" class="btn btn-warning">{{name}}</button>
        {{type}}
        <br>
    </li>


    {{/each}}
</ul>




</div>